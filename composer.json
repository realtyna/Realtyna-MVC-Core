{
  "name": "realtyna/mvc-core",
  "description": "Core library to create WordPress plugin in MVC design pattern",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Realtyna\\MvcCore\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Realtyna",
      "email": "info@realtyna.net"
    },
    {
      "name": "Chandler P",
      "email": "chandler.p@realtyna.net"
    },
    {
      "name": "Anton E",
      "email": "anton.e@realtyna.com"
    },
    {
      "name": "Alan B",
      "email": "alan.b@realtyna.net"
    }
  ],
  "require": {
    "php": ">=7.4",
    "robmorgan/phinx": "^0.13.4",
    "illuminate/database": "^8.83",
    "firebase/php-jwt": "^6.3",
    "ext-json": "*",
    "php-di/php-di": "^6.4",
    "doctrine/annotations": "^1.14.2",
    "htmlburger/carbon-fields-plugin": "^3.3",
    "illuminate/pagination": "^8.83",
    "monolog/monolog": "^2.9"
  },
  "require-dev": {
    "symfony/var-dumper": "^v5.4.0",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": false
    }
  }
}
